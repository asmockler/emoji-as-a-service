{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": ".",
    "jsx": "react",
    "module": "commonjs",
    "allowJs": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "*": ["*"]
    }
  },
  "include": ["./config/typescript", "./functions", "./docs/**/*"]
}
